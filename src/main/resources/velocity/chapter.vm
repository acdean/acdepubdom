<?xml version='1.0' encoding='utf-8'?>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<!-- generated by acdepub ${date} -->
#if($info.normal)
<title>${book.tocTitle} - ${info.numbering}</title>
#else
<title>${book.tocTitle}</title>
#end
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
<link href="stylesheet.css" rel="stylesheet" type="text/css"/>
</head>
<body class="text">
#if($info.normal)
## chapter or part_chapter
<h2>
#if($info.numbering)
<span class="chapterNumber">${info.numbering}</span>
#if($info.title)
<br/>
#end
#end
#if($info.title)
<span class="chapterTitle">${info.title}</span>
#end
</h2>
#else
## prefixes etc - no numbers
#if($info.title)
<h2>
<span class="chapterTitle">${info.title}</span>
</h2>
#end
#end
${info.contents}
</body>
</html>
